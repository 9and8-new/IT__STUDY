package Ch02;

// this
// 클래스 내에서 사용되는 예약어
// 생서오디는 객체의 위치정보(메모리주소-해시코드값)을 확인하는데 사용

// this : 멤버변수와 지역변수 구별에 사용
// this : 다른생성자 호출 사용

class C03Simple
{
	//속성
	int x;
	int y;
	
	//생성자 만들기
	C03Simple(){
		System.out.println("");
	}
	C03Simple(int x){
		System.out.println("");
	}
	C03Simple(int x, int y){
		System.out.println("");
	}
	
	//기능
	public C03Simple getThis() {
		return this;
	}
}
public class C03This {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		C03Simple ob1 = new C03Simple();
		System.out.println(ob1); //Ch02.C03Simple@5305068a
		//Ch02(클래스명).C03Simple()@5305068a(JVM?)
		System.out.println(ob1.getThis());

		C03Simple ob2 = new C03Simple();
		System.out.println(ob2);
		System.out.println(ob2.getThis());
	}

}
