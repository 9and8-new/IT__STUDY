		//2단 - 9단
		//9단 - 2단
		int dan = 9;
		while(dan<9 && dan>1) 
		{
			int i = dan-1;
			while(i<10) 
			{
				System.out.printf("%d * %d = %d\n", dan, i, dan*i);
				i++;
			}
			dan--;
		}
		//2단 - n단(n>2)
		//n단 - m단(n<m)

	}

}
